HOST=0.0.0.0
PORT=1337
APP_KEYS="toBeModified1,toBeModified2"
API_TOKEN_SALT=tobemodified
ENCRYPTION_KEY=tobemodified
ADMIN_JWT_SECRET=tobemodified
TRANSFER_TOKEN_SALT=tobemodified
JWT_SECRET=tobemodified

# For large/slow imports with SQLite (avoids KnexTimeoutError). Value in ms, e.g. 600000 = 10 min
# DATABASE_CONNECTION_TIMEOUT=600000

# Directory data import (scripts/data_import_seed_directory_mosc_in.js)
# Both are read from .env by the script (dotenv).
STRAPI_DATA_IMPORT_PROJECT_CLONE_DIR=E:\project_workspace\directory-mosc-in-temp
TENANT_ID=directory_mosc_001
# Optional: set to 1 to fetch priest/church list pages from live site when not in clone
# STRAPI_DIRECTORY_FETCH_MISSING_PAGES=0

# REST push to Cloud (scripts/rest_api_push_to_cloud.js). Omit or set to 1 to push media and link cover/Bishop image.
# REST_PUSH_INCLUDE_UPLOADS=1
# Set to 0 only to skip Phase 0 (cover/image will stay empty):
# REST_PUSH_INCLUDE_UPLOADS=0

# Clear / keep-five scripts: target remote (Cloud) by default; use --local to target local Strapi.
# STRAPI_CLOUD_URL=https://YOUR-PROJECT.strapiapp.com
# STRAPI_CLOUD_API_TOKEN=your-full-access-token
# For --local: STRAPI_LOCAL_URL (default http://localhost:1337), STRAPI_LOCAL_API_TOKEN (Full Access from local admin)
# STRAPI_LOCAL_URL=http://localhost:1337
# STRAPI_LOCAL_API_TOKEN=

# S3 upload (when using config/env/development/plugins.js or config/env/production/plugins.js)
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_REGION=us-east-2
# AWS_S3_BUCKET_NAME=eventapp-media-bucket
